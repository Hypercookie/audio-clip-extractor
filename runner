import audioextractor.audio_extractor as audio_extractor
import sys
from os.path import basename

if len(sys.argv) < 3:
    print("usage: %s <audioFilePath> <labelsFilePath> [outputDir]" % basename(__file__))
    sys.exit(2)

audio_extractor.run(sys.argv[1], sys.argv[2], sys.argv[3] if len(sys.argv) >= 4 else None)
